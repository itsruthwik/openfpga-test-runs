                  <pb_type name="ff" blif_model=".latch" num_pb="1" class="flipflop">
                     <input name="D" num_pins="1" port_class="D"/>
                     <output name="Q" num_pins="1" port_class="Q"/>
                     <clock name="clk" num_pins="1" port_class="clock"/>
                     <T_setup value="66e-12" port="ff.D" clock="clk"/>
                     <T_clock_to_Q max="124e-12" port="ff.Q" clock="clk"/>
                  </pb_type>
                  <interconnect>
                     <direct name="direct1" input="ble6.in" output="lut6[0:0].in"/>
                     <direct name="direct2" input="lut6.out" output="ff.D">
                        <pack_pattern name="ble6" in_port="lut6.out" out_port="ff.D"/>
                     </direct>
                     <direct name="direct3" input="ble6.clk" output="ff.clk"/>
                     <mux name="mux1" input="ff.Q lut6.out" output="ble6.out">
                        <delay_constant max="25e-12" in_port="lut6.out" out_port="ble6.out"/>
                        <delay_constant max="45e-12" in_port="ff.Q" out_port="ble6.out"/>
                     </mux>
                  </interconnect>
               </pb_type>
               <interconnect>
                  <direct name="direct1" input="fle.in[5:0]" output="ble6.in"/>
                  <direct name="direct2" input="ble6.out" output="fle.out[0:0]"/>
                  <direct name="direct3" input="fle.clk" output="ble6.clk"/>
               </interconnect>
            </mode>
            <!-- end of physical mode -->
            <!-- 6-LUT mode definition begin -->
            <mode name="n1_lut6">
               <!-- Define 6-LUT mode -->
               <pb_type name="ble6" num_pb="1">
                  <input name="in" num_pins="6"/>
                  <output name="out" num_pins="1"/>
                  <clock name="clk" num_pins="1"/>
                  <!-- Define LUT -->
                  <pb_type name="lut6" blif_model=".names" num_pb="1" class="lut">
                     <input name="in" num_pins="6" port_class="lut_in"/>
                     <output name="out" num_pins="1" port_class="lut_out"/>
                     <delay_matrix type="max" in_port="lut6.in" out_port="lut6.out">
                  261e-12
                  261e-12
                  261e-12
                  261e-12
                  261e-12
                  261e-12
                     </delay_matrix>
                  </pb_type>
                  <pb_type name="ff" blif_model=".latch" num_pb="1" class="flipflop">
                     <input name="D" num_pins="1" port_class="D"/>
                     <output name="Q" num_pins="1" port_class="Q"/>
                     <clock name="clk" num_pins="1" port_class="clock"/>
                     <T_setup value="66e-12" port="ff.D" clock="clk"/>
                     <T_clock_to_Q max="124e-12" port="ff.Q" clock="clk"/>
                  </pb_type>
                  <interconnect>
                     <direct name="direct1" input="ble6.in" output="lut6[0:0].in"/>
                     <direct name="direct2" input="lut6.out" output="ff.D">
                        <pack_pattern name="ble6" in_port="lut6.out" out_port="ff.D"/>
                     </direct>
                     <direct name="direct3" input="ble6.clk" output="ff.clk"/>
                     <mux name="mux1" input="ff.Q lut6.out" output="ble6.out">
                        <delay_constant max="25e-12" in_port="lut6.out" out_port="ble6.out"/>
                        <delay_constant max="45e-12" in_port="ff.Q" out_port="ble6.out"/>
                     </mux>
                  </interconnect>
               </pb_type>
               <interconnect>
                  <direct name="direct1" input="fle.in[5:0]" output="ble6.in"/>
                  <direct name="direct2" input="ble6.out" output="fle.out[0:0]"/>
                  <direct name="direct3" input="fle.clk" output="ble6.clk"/>
               </interconnect>
            </mode>
            <!-- n1_lut6 -->
         </pb_type>
         <interconnect>
            <complete name="crossbar" input="clb.I fle[9:0].out" output="fle[9:0].in">
               <delay_constant max="95e-12" in_port="clb.I" out_port="fle[9:0].in"/>
               <delay_constant max="75e-12" in_port="fle[9:0].out" out_port="fle[9:0].in"/>
            </complete>
            <complete name="clks" input="clb.clk" output="fle[9:0].clk">
            </complete>
            <direct name="clbouts1" input="fle[9:0].out" output="clb.O[9:0]"/>
         </interconnect>
      </pb_type>

    <pb_type name="router">
      <!-- input output ports start   -->
         <clock name="clk" num_pins="1"/>
         <input name="reset"  num_pins="1"/>  
         <input name="router_address"  num_pins="4"/>  
         <input name="channel_in_ip"  num_pins="350"/>  
         <input name="flow_ctrl_in_op"  num_pins="15"/>   
         
         <output name="error"  num_pins="1"/>  
         <output name="channel_out_op"  num_pins="350"/>  
         <output name="flow_ctrl_out_ip"  num_pins="15"/>  
      <!-- input output ports end  -->
      <mode name="router_wrap">
         <pb_type name="router_wrap" blif_model=".subckt router_slice" num_pb="1">
            <!-- input output ports  start   -->
               <clock name="clk" num_pins="1"/>
               <input name="reset"  num_pins="1"/>  
               <input name="router_address"  num_pins="4"/>  
               <input name="channel_in_ip"  num_pins="350"/>  
               <input name="flow_ctrl_in_op"  num_pins="15"/>   

               <output name="error"  num_pins="1"/>  
               <output name="channel_out_op"  num_pins="350"/>  
               <output name="flow_ctrl_out_ip"  num_pins="15"/>  
            <!-- input output ports  end   -->
            <!-- seq delay start -->
              <T_setup value="66e-12" port="router_wrap.reset" clock="clk"/>  
              <T_setup value="66e-12" port="router_wrap.router_address" clock="clk"/>  
              <T_setup value="66e-12" port="router_wrap.channel_in_ip" clock="clk"/>  
              <T_setup value="66e-12" port="router_wrap.flow_ctrl_in_op" clock="clk"/>   

              <T_setup value="66e-12" port="router_wrap.error" clock="clk"/>  
              <T_setup value="66e-12" port="router_wrap.channel_out_op" clock="clk"/>  
              <T_setup value="66e-12" port="router_wrap.flow_ctrl_out_ip" clock="clk"/>  

              <T_clock_to_Q max="124e-12" port="router_wrap.reset" clock="clk"/>
              <T_clock_to_Q max="124e-12" port="router_wrap.router_address" clock="clk"/>
              <T_clock_to_Q max="124e-12" port="router_wrap.channel_in_ip" clock="clk"/>
              <T_clock_to_Q max="124e-12" port="router_wrap.flow_ctrl_in_op" clock="clk"/>

              
              <T_clock_to_Q max="124e-12" port="router_wrap.error" clock="clk"/>
              <T_clock_to_Q max="124e-12" port="router_wrap.channel_out_op" clock="clk"/>
              <T_clock_to_Q max="124e-12" port="router_wrap.flow_ctrl_out_ip" clock="clk"/>
            <!-- Seq delay end -->
         </pb_type>
        <interconnect>
          <direct name="rst_2rst_" input="router.reset" output="router_wrap.reset"/>
          <direct name="my_xpos2my_xpos" input="router.router_address" output="router_wrap.router_address"/>
          <direct name="my_ypos2my_ypos" input="router.channel_in_ip" output="router_wrap.channel_in_ip"/>
          <direct name="idata_02idata_0" input="router.flow_ctrl_in_op" output="router_wrap.flow_ctrl_in_op"/>
          
          <direct name="odata_22odata_2" input="router_wrap.error" output="router.error"/>
          <direct name="odata_32odata_3" input="router_wrap.channel_out_op" output="router.channel_out_op"/>
          <direct name="odata_42odata_4" input="router_wrap.flow_ctrl_out_ip" output="router.flow_ctrl_out_ip"/>
          <direct name="clk" input="router.clk" output="router_wrap.clk"/>
        </interconnect>
      </mode>
      <power method="sum-of-children"/>
    </pb_type>
    
   </complexblocklist>

